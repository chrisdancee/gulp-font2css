{
  "name": "gulp-font2css",
  "version": "0.2.0",
  "description": "Encode font files as CSS using Gulp.",
  "repository": "chrisdancee/gulp-font2css",
  "author": {
    "name": "Lu√≠s Rodrigues",
    "email": "hello@goblindegook.com",
    "url": "http://goblindegook.com"
  },
  "license": "MIT",
  "main": "lib/index.js",
  "scripts": {
    "build": "cd src; babel *.js -d ../lib",
    "docs": "esdoc -c esdoc.json",
    "prepublish": "npm run build",
    "pretest": "eslint src/**/*.js",
    "test": "ava --require babel-core/register"
  },
  "keywords": [
    "base64",
    "css",
    "font-face",
    "gulpplugin",
    "localfont",
    "otf",
    "ttf",
    "web",
    "webfonts",
    "woff",
    "woff2"
  ],
  "devDependencies": {
    "ava": "^0.9.1",
    "babel-cli": "^6.3.17",
    "babel-core": "^6.3.26",
    "babel-preset-es2015": "^6.3.13",
    "css": "^2.2.1",
    "eslint": "^1.10.3"
  },
  "dependencies": {
    "css-font-style-keywords": "^1.0.1",
    "css-font-weight-keywords": "^1.0.0",
    "css-font-weight-names": "^0.2.1",
    "gulp-util": "^3.0.7",
    "mime": "^1.3.4",
    "through2": "^2.0.0"
  }
}
